datasource db {
  provider = "sqlite"
  url      = "file:../data/dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Plan {
  id        String   @id @default(cuid())
  createdAt DateTime @default(now())
  title     String
  titlesStr String        // "A|B|C"
  itemsJson String        // JSON string of Item[]
}
